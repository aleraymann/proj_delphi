  object QryClientesFicha: TZQuery
    Connection = dtmPrincipal.ConexaoDB
    Active = True
    SQL.Strings = (
      'select clientes.clienteId,'
      '      nome,'
      '      cep,'
      '      endereco,'
      '      bairro,'
      '      cidade,'
      '      estado,'
      '      email,'
      '      telefone,'
      '      dataNascimento'
      'from clientes'
      'order by nome')
